<%- include('header') %>


            <!-- MAIN CONTENT-->
            <div class="main-content">
                <div class="section__content section__content--p30">
                    <div class="container-fluid" style="text-align: center;">
                        <div class="row">
                            <div class="col-lg-12">
                                <form class="form-header m-b-30">
                                    <input class="au-input au-input--xl" value="<%= search %>" type="text" name="search" placeholder="Search for name and email" />
                                    <button class="au-btn--submit" type="submit">
                                        <i class="zmdi zmdi-search"></i>
                                    </button>
                                    <button class="au-btn--submit" style="margin-left: 20px;">
                                        <a href="/admin/contact-us/view_message" style="color: white; padding: 0 20px;">Reset</a>
                                    </button>
                                </form>
                                <div class="table-responsive table--no-card m-b-30">
                                    <form action="/admin/contact-us/deletemulticontat" method="post">
                                        <table class="table table-borderless table-striped table-earning">
                                            <thead>
                                                <tr>
                                                    <th>
                                                        <input type="checkbox" id="CheckAll">
                                                        <button type="submit" style="margin-left: 5px; color: white;"><i class="fa fa-trash"></i></button>
                                                    </th>
                                                    <th>No.</th>
                                                    <th>Name</th>
                                                    <th>Email</th>
                                                    <th>Subject</th>
                                                    <th>Message</th>
                                                    <th>Created Date</th>
                                                    <th>Status</th>
                                                    <th>Action</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <% var i=1 %>
                                                <% for(cdata of cntdata){ %> 
                                                    <tr>
                                                        <td><input type="checkbox" value="<%= cdata.id %>" name="offerids[]"></td>
                                                        <td><%= i++ %></td>
                                                        <td><%= cdata.name %></td>
                                                        <td><%= cdata.email %></td>
                                                        <td><%= cdata.subject %></td>
                                                        <td><%= cdata.message.substr(0,20) %>...</td>
                                                        <td><%= cdata.create_date %></td>
                                                        <td>
                                                            <% if(cdata.status){ %> 
                                                                <a href="/admin/contact-us/deactive/<%= cdata.id %>" class="text-danger">Pending</a>
                                                            <% }else{ %> 
                                                                <a class="text-success">Done</a>
                                                            <% } %>
                                                        </td>
                                                        <td>
                                                            <a href="/admin/contact-us/deletemessage/<%= cdata.id %>">
                                                                <i class="fa fa-trash"></i>
                                                            </a> ||
                                                            <a href="/admin/contact-us/viewcontact/<%= cdata.id %>">
                                                                <i class="fa fa-eye"></i>
                                                            </a>
                                                        </td>
                                                    </tr>
                                                <% } %>
                                            </tbody>
                                        </table>
                                    </form>
                                </div>
                            </div>
                        </div>

                        <%- include('pegination') %>

<%- include('footer') %>