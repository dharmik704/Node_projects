<%- include('header') %>

            <!-- Table Start -->
            <center>
                <div class="container-fluid pt-4 px-4">
                    <img src="<%= locals.admin.image %>" alt="image" width="150" style="border-radius: 50%; margin-bottom: 20px;">
                    <div class="row g-4" style="justify-content: center;">
                        <div class="col-sm-12 col-xl-6">
                            <div class="bg-light rounded h-100 p-4">
                                <h6 class="mb-4">Your Profile</h6>
                                <table class="table table-hover">
                                    <tbody>
                                        <tr>
                                            <th scope="row">Name</th>
                                            <td><%= locals.admin.name %></td>
                                        </tr>
                                        <tr>
                                            <th scope="row">Email</th>
                                            <td><%= locals.admin.email %></td>
                                        </tr>
                                        <tr>
                                            <th scope="row">Gender</th>
                                            <td><%= locals.admin.gender %></td>
                                        </tr>
                                        <tr>
                                            <th scope="row">Hobby</th>
                                            <td><%= locals.admin.hobby %></td>
                                        </tr>
                                        <tr>
                                            <th scope="row">City</th>
                                            <td><%= locals.admin.city %></td>
                                        </tr>
                                        <tr>
                                            <th scope="row">Address</th>
                                            <td><%= locals.admin.address %></td>
                                        </tr>
                                        <tr>
                                            <th scope="row">Role</th>
                                            <td><%= locals.admin.role %></td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </center>
            <!-- Table End -->

<%- include('footer') %>