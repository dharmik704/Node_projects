    <script>

        <% if(locals.flash.success.length > 0 && locals.flash.success){ %> 
        
            new Noty({
                type: 'success',
                layout: 'topRight',
                text: '<%= locals.flash.success %>',
                timeout: 1000,
                theme: 'metroui'
            }).show();

        <% } %>

        <% if(locals.flash.error.length > 0 && locals.flash.error){ %> 
        
            new Noty({
                type: 'error',
                layout: 'topRight',
                text: '<%= locals.flash.error %>',
                timeout: 1000,
                theme: 'metroui'
        }).show();

    <% } %>

    $("#CheckAll").click(function(){
    $('input:checkbox').not(this).prop('checked', this.checked);
    });

    </script>

